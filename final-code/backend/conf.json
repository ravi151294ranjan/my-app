{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [
            "."
        ],
        "exclude": [
            "node_modules","public","others","Netapp","uploads"
        ]
    },
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "readme": "./readme/readme.md"
    }
}